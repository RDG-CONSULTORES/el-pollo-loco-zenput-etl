{
  "name": "el-pollo-loco-railway-dashboard",
  "version": "1.0.0",
  "description": "El Pollo Loco CAS Dashboard - Railway PostgreSQL",
  "main": "server-fixed.js",
  "scripts": {
    "start": "node server-fixed.js",
    "dev": "nodemon server.js",
    "deploy": "railway deploy",
    "migrate": "node ../migrate_to_railway.js",
    "test": "node test/health-check.js"
  },
  "keywords": [
    "el-pollo-loco",
    "dashboard", 
    "railway",
    "postgresql",
    "cas",
    "supervision"
  ],
  "author": "Roberto Davila",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "railway": {
    "build": {
      "command": "npm install"
    },
    "start": {
      "command": "npm start"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/el-pollo-loco-railway-dashboard"
  },
  "environment": {
    "NODE_ENV": "production",
    "PORT": "${{PORT}}",
    "DATABASE_URL": "${{Postgres.DATABASE_URL}}"
  }
}